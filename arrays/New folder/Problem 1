int main(void) {

    int arr[10];
    int i, j;
    
    printf("Input 10 elements");
    
      for (i = 0; i < 10; i++)
          scanf("%d", &j)
          arr[i] = j;
          
      for (i = 0; i < 10; i++)
      {
      
          printf("element - %d :", i);
          printf("%d", arr[i]);
          printf("\n");
          }   
    


    }
